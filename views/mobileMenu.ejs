<div class="mobileMenu" style="display:none">
  <ul class="list-unstyled">
    <% brands.objects.map(brand => { %>
        <li class="mobileMenu-item" key={brand.slug}><a href="/brand/<%= brand.slug %>"><%= brand.title %></a></li>
